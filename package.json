{
  "name": "google4Watson",
  "description": "A sample of Google Home Integration",
  "version": "0.0.1",
  "private": true,
  "license": "Apache Version 2.0",
  "author": "Vincent Perrin",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "test": "npm run lint-md && npm run eslint",
    "eslint": "eslint *.js lib",
    "eslint-fix": "eslint --fix *.js lib",
    "lint-md": "./node_modules/.bin/remark -f *.md doc/source/*.md"
  },
  "remarkConfig": {
    "plugins": [
      "remark-validate-links",
      "remark-lint-no-dead-urls"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "ibm-watson": "4.5.1",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2",
    "remark-cli": "^7.0.0",
    "remark-lint": "^6.0.5",
    "remark-lint-no-dead-urls": "^1.0.0",
    "remark-validate-links": "^9.0.1"
  }
}
